
package recapdemo1;

public class RecapDemo1 {

    public static void main(String[] args) {
       int sayi1=200;
       int sayi2=250;
       int sayi3=206;
       int enbüyük=sayi1;
       
      
       if(enbüyük<sayi2){
           enbüyük=sayi2;
       }
       if(enbüyük<sayi3){
           enbüyük=sayi3;
       }
        System.out.println("en büyük sayı:" +enbüyük);
    }
    
}
